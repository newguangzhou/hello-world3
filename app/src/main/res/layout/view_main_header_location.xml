<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:auto="http://schemas.android.com/apk/res-auto"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="35dp"
    android:layout_height="35dp"
    android:layout_centerHorizontal="true"
    android:orientation="vertical">

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/map_pet_loca_avater"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:scaleType="fitXY"
        fresco:actualImageScaleType="focusCrop"
        fresco:fadeDuration="300"

        fresco:failureImage="@drawable/tab_health_head_defult"
        fresco:failureImageScaleType="centerInside"


        fresco:placeholderImage="@drawable/tab_health_head_defult"
        fresco:placeholderImageScaleType="fitCenter"
        fresco:roundedCornerRadius="16dp" />

    <com.facebook.drawee.view.SimpleDraweeView
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:scaleType="fitXY"
        fresco:actualImageScaleType="focusCrop"
        fresco:placeholderImage="@drawable/header_circle"
        fresco:placeholderImageScaleType="fitCenter"></com.facebook.drawee.view.SimpleDraweeView>

</FrameLayout>